          <TYPE>QUA</TYPE>
        </macros>
        <group_name></group_name>
      </tab>
      <tab>
        <name>Correctors</name>
        <file>magnet-array-main.bob</file>
        <macros>
          <TYPE>COR</TYPE>
        </macros>
        <group_name></group_name>
      </tab>
      <tab>
        <name>Solenoids</name>
        <file>magnet-array-main.bob</file>
        <macros>
          <TYPE>SOL</TYPE>
        </macros>
        <group_name></group_name>
      </tab>
    </tabs>
    <x>1</x>
    <y>40</y>
    <width>699</width>
    <height>740</height>
    <direction>0</direction>
  </widget>
